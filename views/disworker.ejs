<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Log Data</title>
  <!-- Bootstrap CSS link -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
  <style>
    body {
      padding: 20px;
    }
    table {
      width: 100%;
      margin-top: 20px;
    }
    th, td {
      padding: 10px;
      text-align: center;
    }
  </style>
</head>
<body>
  <div class="container">
    <h1 class="mt-4 mb-4">Worker Data</h1>
    <table class="table table-bordered">
      <thead>
        <tr>
          <th>Worker ID</th>
          <th>Name</th>
          <th>CNIC</th>
          <th>Email</th>
          <th>Price Per Hour</th>
          <th>Service type</th>
        </tr>
      </thead>
      <tbody>
        <% result.forEach(function(entry) { %>
          <tr>
            <td><%= entry.wid %></td>
            <td><%= entry.name %></td>
            <td><%= entry.cnic %></td>
            <td><%= entry.email %></td>
            <td><%= entry.priceperhour %></td>
            <td><%= entry.servicetype %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </div>
  <!-- Bootstrap JS and Popper.js scripts (for Bootstrap features) -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>